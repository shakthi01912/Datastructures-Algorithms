   "source": [
    "arr = [1, 2, 3, 4, 5, 6, 7, 8, 9]\n",
    "k = 3 \n",
    "\n",
    "def reverse():\n",
    "   \n",
    "    n = len(arr)\n",
    "    \n",
    "    for i in range(0, n, k):\n",
    "        arr[i:i+k] = arr[i:i+k][::-1]\n",
    "\n",
    "    return arr\n",
    "\n",
    "reverse()"
   ]
  }
